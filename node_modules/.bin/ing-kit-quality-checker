#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../ing-kit-quality-checker/bin/ing-kit-quality-checker.js" "$@"
  ret=$?
else 
  node  "$basedir/../ing-kit-quality-checker/bin/ing-kit-quality-checker.js" "$@"
  ret=$?
fi
exit $ret
